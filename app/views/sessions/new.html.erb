<div class="container white-text alt-font">

  <h1 class="main-font">Sign In</h1>
  <div class="col-md-6">
    <%= form_for :sessions, url: login_path, :html => {:autocomplete => "off"} do |f| %>
        <%= f.label :email %>
        <%= f.text_field :email, class: "form-control" %>
          <br>
        <%= f.label :password %>
        <%= f.text_field :password, type: "password", class: "form-control" %>
          <br>
        <%= f.submit 'Log In', class: "btn btn-default" %>
    <% end %>
  </div>

</div>